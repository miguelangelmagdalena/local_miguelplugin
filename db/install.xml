<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/miguelplugin/db" VERSION="20230203" COMMENT="XMLDB file for Moodle local/miguelplugin"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_miguelplugin" COMMENT="Tabla de usuarios">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="nombre" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" COMMENT="nombre del usuario"/>
        <FIELD NAME="apellido" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" COMMENT="Apellido del usuario"/>
        <FIELD NAME="edad" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
